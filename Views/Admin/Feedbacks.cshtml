@{
    Layout = "_LayoutAdmin";
    ViewData["title"] = "Notifications";
}

@model IEnumerable<NGODP.Models.Feedback>

<div class="container">

    <div class="row mb-5">
        <span class="h2 text-primary mb-3">Feedbacks</span>
    </div>

    @foreach (Feedback x in Model)
    {
        <div class="row bg-light mb-2">
            <div class="media-body pb-3 mb-0 lh-125 border-bottom border-black">
                <div class="d-flex justify-content-between w-100">
                <span class="ml-3 mr-4 pt-2 text-muted" href="#"><i>@x.TimeAndDate</i></span>
                </div>
                <div class="mt-3 mb-3 ">
                    <strong class="ml-3 mb-5">@x.Message</strong>
                </div>
          </div>
        </div>
    }

</div>